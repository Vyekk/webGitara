{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "outDir": "./dist",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "typeRoots": ["./src/types", "./node_modules/@types"]
    },
    "include": ["src/**/*.ts", "app.ts"],
    "exclude": ["node_modules"]
}
